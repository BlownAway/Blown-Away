<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      android:versionCode="1"
      android:versionName="1.0" package="hopur20.blownaway">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".MainMenu" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity class=".Game" android:name="Game">
			<intent-filter>
				<action android:name="hopur20.blownaway.Game.View" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity class=".GameManager" android:name="GameManager">
			<intent-filter>
				<action android:name="hopur20.blownaway.GameManager.View" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>

    
</application>


</manifest> 